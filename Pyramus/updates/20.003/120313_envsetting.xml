<?xml version="1.0" encoding="UTF-8"?>
<update xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.ofw.fi/xml/2011/java-xmldb-updater/UpdaterSchema.xsd">

  <alterData>
    <insert table="SettingKey">
      <value type="java.lang.Long" column="id" value="21"/>
      <value type="java.lang.String" column="name" value="system.environment"/>
    </insert>
    <insert table="Setting">
      <value type="java.lang.Long" column="id" value="21"/>
      <value type="java.lang.Long" column="settingKey" value="21"/>
      <value type="java.lang.String" column="value" value="production"/>
    </insert>
    <update table="SettingKey">
      <set>
        <value type="java.lang.Long" column="sequence_next_hi_value" value="22"/>
      </set>
      <where>
        <equals type="java.lang.String" column="sequence_name" value="SettingKey"/>
      </where>
    </update>
    <update table="Setting">
      <set>
        <value type="java.lang.Long" column="sequence_next_hi_value" value="22"/>
      </set>
      <where>
        <equals type="java.lang.String" column="sequence_name" value="Setting"/>
      </where>
    </update>
  </alterData>
</update>